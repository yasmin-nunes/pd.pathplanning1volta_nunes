# posição inicial do carro
car_position =[0, 0]


def distancia_carro(point1, point2): #vai pegar e substituir ali dentro quando for retomada
    return np.sqrt((point1[0] - point2[0]) ** 2 + (point1[1] - point2[1]) ** 2) #pitagoras para ver a distancia


distancia_maxima = 20.0
filtrada_data = [] #criar uma nova lista com os pontos que estão próximos o suficienete do ref
for d in data:
    if distancia_carro(car_position, d[:2]) <= distancia_maxima: #pegar os elementos em x e y 
        filtrada_data.append(d)

# separando as coordenadas dos dados filtrados
x_filtrado = [d[0] for d in filtrada_data]
y_filtrado = [d[1] for d in filtrada_data]
confidence_filtrado = [d[2] for d in filtrada_data]
color_filtrado = [d[3] for d in filtrada_data]

filtrada_data_array = np.array(filtrada_data)

tri = Delaunay(filtrada_data_array[:, :2])

# determinando o tamanho da tela e os pontos em scatter
plt.figure(figsize=(7, 5)) #tamanho da janela

scatter = plt.scatter(x_filtrado, y_filtrado, s=[c * 50 for c in confidence_filtrado], c=color_filtrado, cmap='viridis', alpha=1) #grafico de dispersão

plt.triplot(filtrada_data_array[:, 0], filtrada_data_array[:, 1], tri.simplices, linewidth=0.4)

plt.plot(filtrada_data_array[:, 0], filtrada_data_array[:, 1], 'o', alpha=0.0)

pontos_medios = []

# itinerar entre os triangulos feitos na triangulação

for simplex in tri.simplices:
    v1, v2, v3 = simplex
    # pontos médios -> triangulos dentro de triangulos
    ponto_medio1 = ((filtrada_data_array[v1][0] + filtrada_data_array[v2][0]) / 2, 
                        (filtrada_data_array[v1][1] + filtrada_data_array[v2][1]) / 2)
    
    ponto_medio2 = ((filtrada_data_array[v2][0] + filtrada_data_array[v3][0]) / 2, 
                        (filtrada_data_array[v2][1] + filtrada_data_array[v3][1]) / 2)
    
    ponto_medio3 = ((filtrada_data_array[v3][0] + filtrada_data_array[v1][0]) / 2, 
                    (filtrada_data_array[v3][1] + filtrada_data_array[v1][1]) / 2)
    
    pontos_medios.extend([ponto_medio1, ponto_medio2, ponto_medio3])

pontos_medios_array = np.array(pontos_medios)

plt.plot(pontos_medios_array[:, 0], pontos_medios_array[:, 1], "^", alpha = 1)

#posição do carro 

def adicionar_seta_com_yaw(car_position, yaw, comprimento_seta=1.5): #cordenadas do vrum vrum 
    delta_x = comprimento_seta * np.cos(yaw) 
    delta_y = comprimento_seta * np.sin(yaw)
    seta_x = car_position[0] + delta_x
    seta_y = car_position[1] + delta_y
    
    # adicionar a seta ao gráfico
    plt.annotate('', xy=(seta_x, seta_y), xycoords='data',
                 xytext=car_position, textcoords='data',
                 arrowprops=dict(facecolor='black', shrink=0.05),
                 fontsize=12, color='black')


yaw = np.pi  # angulo de yaw em radianos (45 graus)

# adicionar a seta com base no yaw
adicionar_seta_com_yaw(car_position, yaw)

plt.plot(car_position[0], car_position[1], 'o') 


# o que vai mostrar no gráfico além dos pontos 
cbar = plt.colorbar(scatter)
cbar.set_label('cor dos cones')
plt.xlabel('eixo x')
plt.ylabel('eixo y')
plt.title('pista filtrada!')

plt.show() #plotando o gráfico
